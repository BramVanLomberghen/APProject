<p id="notice"><%= notice %></p>

<h1>Listing Student Exams</h1>

<table>
  <thead>
    <tr>
      <th>Student</th>
      <th>Course</th>
      <th>Supervision</th>
      <th>Start</th>
      <th>End</th>
      <th>Present</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @student_exams.each do |student_exam| %>
      <tr>
        <td><%= student_exam.student_id %></td>
        <td><%= student_exam.course_id %></td>
        <td><%= student_exam.supervision_id %></td>
        <td><%= student_exam.start %></td>
        <td><%= student_exam.end %></td>
        <td><%= student_exam.present %></td>
        <td><%= link_to 'Show', student_exam %></td>
        <td><%= link_to 'Edit', edit_student_exam_path(student_exam) %></td>
        <td><%= link_to 'Destroy', student_exam, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Student exam', new_student_exam_path %>
